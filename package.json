{
    "name": "@dlicheva/auth",
    "version": "1.0.0",
    "description": "An alternative module to @nuxtjs/auth",
    "homepage": "https://github.com/dlicheva/auth",
    "author": "Denoder, dlicheva",
    "keywords": [
        "auth",
        "nuxt",
        "nuxt4",
        "nuxtjs",
        "nuxt-module",
        "nuxt-plugin",
        "@nuxtjs/auth",
        "@dlicheva/auth"
    ],
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "module": "./dist/module.mjs",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "JITI_ESM_RESOLVE=1 jiti ./commands/cli.ts build --stub && JITI_ESM_RESOLVE=1 jiti ./commands/cli.ts prepare",
        "prepack": "JITI_ESM_RESOLVE=1 jiti ./commands/cli.ts build"
    },
    "dependencies": {
        "@nuxt-alt/http": "latest",
        "@refactorjs/serialize": "latest",
        "cookie-es": "^2.0.0",
        "defu": "^6.1.4",
        "jwt-decode": "^4.0.0",
        "ohash": "^2.0.11",
        "pathe": "^2.0.3",
        "pinia": "^3.0.3",
        "requrl": "^3.0.2",
        "@nuxt/ui": "^3.3.4"
    },
    "devDependencies": {
        "@nuxt/schema": "^4.1.2",
        "@types/node": "^24",
        "jiti": "^2.5.1",
        "nuxt": "^4.1.2",
        "typescript": "^5.9.2",
        "unbuild": "^3.6.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dlicheva/auth.git",
        "directory": "@dlicheva/auth"
    },
    "bugs": {
        "url": "https://github.com/dlicheva/auth/issues"
    },
    "publishConfig": {
        "access": "public"
    }
}
